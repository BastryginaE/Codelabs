#include <iostream>
#include <cmath>
 
using namespace std;
 
int main()
{
    float a,x,g,f,y;
    cout<<"  x="; cin>>x;
    cout<<"  a="; cin>>a;
    g = -(8*(7 * (a,2) + 34 * a * x - 5 * (x,2))/(27 * pow(a,2) + 33 * a * x + 10 *pow(x,2));
    f = -(1/(sinh(72 * pow(a,2) - 5 * a * x - 12 * pow(x,2) - M_PI/2))); 
    y = log(42 * pow(a,2) + 53 * a * x + 15 * pow(x,2) + 1);
    cout << "g = " << g << endl;
	cout << "f = " << f << endl;
	cout << "y = " << y << endl;
	return 0;
};
